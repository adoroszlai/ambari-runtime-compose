AMBARI-AGENT.INI_agent.alert_grace_period=5
AMBARI-AGENT.INI_agent.alert_kinit_timeout=14400000
AMBARI-AGENT.INI_agent.cache_dir=/var/lib/ambari-agent/cache
AMBARI-AGENT.INI_agent.data_cleanup_interval=86400
AMBARI-AGENT.INI_agent.data_cleanup_max_age=2592000
AMBARI-AGENT.INI_agent.data_cleanup_max_size_mb=100
AMBARI-AGENT.INI_agent.logdir=/var/log/ambari-agent
AMBARI-AGENT.INI_agent.loglevel=INFO
AMBARI-AGENT.INI_agent.parallel_execution=0
AMBARI-AGENT.INI_agent.piddir=/var/run/ambari-agent
AMBARI-AGENT.INI_agent.ping_port=8670
AMBARI-AGENT.INI_agent.prefix=/var/lib/ambari-agent/data
AMBARI-AGENT.INI_agent.run_as_user=root
AMBARI-AGENT.INI_agent.status_command_timeout=5
AMBARI-AGENT.INI_agent.system_resource_overrides=/etc/resource_overrides
AMBARI-AGENT.INI_agent.tolerate_download_failures=true
AMBARI-AGENT.INI_heartbeat.idle_interval_max=10
AMBARI-AGENT.INI_heartbeat.idle_interval_min=1
AMBARI-AGENT.INI_heartbeat.log_lines_count=300
AMBARI-AGENT.INI_heartbeat.state_interval_seconds=60
AMBARI-AGENT.INI_logging.syslog_enabled=0
AMBARI-AGENT.INI_network.use_system_proxy_settings=true
AMBARI-AGENT.INI_security.credential_conf_dir=/var/lib/ambari-agent/cred/conf
AMBARI-AGENT.INI_security.credential_lib_dir=/var/lib/ambari-agent/cred/lib
AMBARI-AGENT.INI_security.credential_shell_cmd=org.apache.hadoop.security.alias.CredentialShell
#AMBARI-AGENT.INI_security.force_https_protocol=PROTOCOL_TLSv1_2
AMBARI-AGENT.INI_security.keysdir=/var/lib/ambari-agent/keys
AMBARI-AGENT.INI_security.passphrase_env_var_name=AMBARI_PASSPHRASE
AMBARI-AGENT.INI_security.server_crt=ca.crt
AMBARI-AGENT.INI_security.ssl_verify_cert=0
AMBARI-AGENT.INI_server.connect_retry_delay=10
AMBARI-AGENT.INI_server.max_reconnect_retry_delay=30
AMBARI-AGENT.INI_server.secured_url_port=8441
AMBARI-AGENT.INI_server.url_port=8440
AMBARI-AGENT.INI_services.pidlookuppath=/var/run/
